{
    "name": "ted.bowman/drupal-scripts",
    "description": "Some commands for working on Drupal, core mostly",
    "type": "library",
    "require": {
        "symfony/console": "^5.2",
        "ext-json": "*",
        "symfony/yaml": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "TedbowDrupalScripts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TedbowDrupalScripts\\Tests\\": "tests/src"
        }
    }
}
